or) {\r\n        if (!stateRef.current) {\r\n          return;\r\n        }\r\n        setLoading(false);\r\n        setError(error.message);\r\n        onError && onError({ type: \"error\", message: `${error.message || error.msg}` });\r\n      });\r\n  }\r\n  //更新缓存数据\r\n  function updateCache(fun) {\r\n    setData(fun(data));\r\n  }\r\n  return { fetch, data, loading, error, setError, updateCache, setLoading };\r\n}\r\nexport function useQuery(ql, params, options) {\r\n  let context = useBaseFetch(ql);\r\n  return {\r\n    ...context,\r\n    fetch: (newParams, newOptions) => {\r\n      context.fetch({ ...params, ...newParams }, { ...options, ...newOptions });\r\n    },\r\n  };\r\n}\r\n\r\nexport function useAutoQuery(ql, params = {}, options = {}) {\r\n  let context = useBaseFetch(ql);\r\n  useEffect(() => {\r\n    //是否等待\r\n    if (options.stop) {\r\n      //停止不需要加载效果\r\n    } else if (options.hold) {\r\n      //hold需要加载效果\r\n      context.setLoading(true);\r\n    } else {\r\n      context.fetch(params, options);\r\n    }\r\n  }, handleOption(params));\r\n  return {\r\n    ...context,\r\n    fetchMore: (newParams, newOptions, updateQuery) => {\r\n      context.fetch({ ...params, ...newParams }, { ...options, ...newOptions, updateQuery });\r\n    },\r\n    update: () => {\r\n      context.fetch(params, options);\r\n    },\r\n  };\r\n}\r\n","G:\\other\\admin\\src\\components\\index\\Bg.js",["391"],"G:\\other\\admin\\src\\components\\index\\Card.js",["392","393","394","395","396","397","398","399","400","401"],"G:\\other\\admin\\src\\page\\signin.js",["402"],"G:\\other\\admin\\src\\uitls\\http.js",[],"G:\\other\\admin\\src\\components\\FormItem.js",[],"G:\\other\\admin\\src\\components\\Transition.js",["403","404"],"import React from \"react\";\r\nimport { TransitionGroup, Transition as ReactTransition } from \"react-transition-group\";\r\n\r\n// The duration for each phase of the transition\r\n// So the total duration will be _twice_ this\r\nconst timeout = 200;\r\